<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.rainng.coursesystem.dao.mapper.CourseMapper">

        <select id="findByRequired" parameterType="com.rainng.coursesystem.dao.mapper.SearchMapper" resultType="com.rainng.coursesystem.dao.mapper.SearchMapper">
            select * from search where 1=1
            <if test="hotel_rank !=null and hotel_rank !=''">
                and ename= #{ename}
            </if>
            <if test="hotel_city !=null and hotel_city !=''">
                and hotel_city= #{hotel_city}
            </if>
            <if test="rm_breakfast_type !=null and rm_breakfast_type !=''">
                and rm_breakfast_type= #{rm_breakfast_type}
            </if>
            <if test="room_type!=null and room_type !=''">
                and room_type= #{room_type}
            </if>

        </select>

    </mapper>